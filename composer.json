{
    "name": "contentnet/oauth2-contentnet",
    "description": "PHP OAuth2 client for ContentNet",
    "type": "library",
    "require": {
        "league/oauth2-client": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "php-coveralls/php-coveralls": "*",
        "squizlabs/php_codesniffer": "*",
        "sensiolabs/security-checker": "^4.1"
    },
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "contentnet"
    ],
    "autoload": {
        "psr-4": {
            "ContentNet\\OAuth2\\Client\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContentNet\\OAuth2\\Client\\Test\\Provider\\": "tests/"
        }
    }
}
